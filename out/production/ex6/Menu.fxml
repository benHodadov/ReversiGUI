<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuController">
   <children>
      <Label layoutX="40.0" layoutY="40.0" text="Menu" underline="true">
         <font>
            <Font name="Berlin Sans FB Bold" size="30.0" />
         </font>
      </Label>
      <Hyperlink fx:id="settingsHyperlink" layoutX="63.0" layoutY="166.0" onMouseClicked="#goSettings" text="Settings">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="20.0" />
         </font>
      </Hyperlink>
      <Hyperlink fx:id="startHyperlink" layoutX="63.0" layoutY="115.0" onMouseClicked="#startGame" text="Play">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="20.0" />
         </font>
      </Hyperlink>
      <Hyperlink fx:id="quitButton" layoutX="500.0" layoutY="350.0" onMouseClicked="#Quit" text="Quit">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="20.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
