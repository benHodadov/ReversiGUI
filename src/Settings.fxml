<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SettingsController">
   <children>
      <Label layoutX="40.0" layoutY="40.0" text="Settings" underline="true">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="59.0" layoutY="116.0" text="First player color" />
      <Label layoutX="59.0" layoutY="166.0" text="Second player color" />
      <Label layoutX="59.0" layoutY="216.0" text="Board size" />
      <Button fx:id="saveButton" layoutX="500.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#save" prefHeight="25.0" prefWidth="55.0" text="Save" />
      <ChoiceBox id="boardSizeBox" fx:id="boardSizeBox" layoutX="189.0" layoutY="212.0" prefHeight="25.0" prefWidth="80.0" />
      <ChoiceBox fx:id="firstPlayerColorBox" layoutX="189.0" layoutY="112.0" prefHeight="25.0" prefWidth="80.0" />
      <ChoiceBox fx:id="secondPlayerColorBox" layoutX="189.0" layoutY="162.0" prefHeight="25.0" prefWidth="80.0" />
      <Button fx:id="backButton" layoutX="40.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="25.0" prefWidth="55.0" text="Back" />
   </children>
</AnchorPane>
